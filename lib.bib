  location = {{Zagreb, HR}},
  url = {https://www.bib.irb.hr/538431},
  abstract = {Prvi dio knjige Povijest znanosti: Vrlo kratak pregled donosi sa\v{z}etu povijest zapadne znanosti od anti\v{c}kih temelja do suvremenosti, s naglaskom na kritikama znanosti s kraja 20. stolje\'ca i promjenama u znanju 21. stolje\'ca. Drugi dio, Retorika znanosti i akademsko pismo, osigurava teorijsko-metodolo\v{s}ke pretpostavke knjige. Tu se raspravlja o terminu, definicijama i klasifikacijama znanosti, o znanstvenim \v{z}anrovima, o temeljnim pojmovima klasi\v{c}ne retorike i suvremenoj retorici znanosti, koja je omogu\'cila definiciju akademskoga pisma i poslu\v{z}ila kao metodi\v{c}ki okvir knjige. Tre\'ci dio, Strategije i tehnike akademskoga pisma, pragmati\v{c}ne je naravi. To je glavni, najopse\v{z}niji dio knjige. Zami\v{s}ljen je kao tehni\v{c}ka pomo\'c u rje\v{s}avanju konkretnih zada\'ca u procesu znanstvenoga istra\v{z}ivanja i pisanja akademske proze. Podijeljen je u sedam poglavlja, a svako je od njih posve\'ceno odre\dj enomu retori\v{c}kom kanonu, dijelu kanona ili tehnikama pisanja: Pripreme za pisanje (Invencija), Struktura i dijelovi (Dispozicija), Odlomak i konektori (Argumentacija), Jezik i stil (Elokucija), Citiranje i citati (Memorija), Dva citatna stila (Velika mehanika) te napokon Posljednji koraci (zavr\v{s}na redakcija teksta). Osim s retori\v{c}kim kanonima studentice i studenti upoznaju se s modusima argumentacije (logos, etos i patos), tehnikama za oblikovanje kair\'os, st\'asis i t\'opoi, a posebna je pozornost posve\'cena citatnim stilovima i mehanici teksta.},
  isbn = {978-953-303-357-0},
  langid = {croatian},
  pagetotal = {681}
}



@article{rincon2017InfluencingPeopleSocial,
  title = {Influencing {{Over People}} with a {{Social Emotional Model}}},
  author = {Rincon, J.A. and de la Prieta, F. and Zanardini, D. and Julian, V. and Carrascosa, C.},
  date = {2017-03},
  journaltitle = {Neurocomputing},
  shortjournal = {Neurocomputing},
  volume = {231},
  pages = {47--54},
  issn = {09252312},
  doi = {10.1016/j.neucom.2016.03.107},
  abstract = {This paper presents an approach of a social emotional model, which allows to extract the social emotion of a group of intelligent entities. The emotional model PAD allows to represent the emotion of an intelligent entity in 3-D space, allowing the representation of different emotional states. The social emotional model presented in this paper uses individual emotions of each one of the entities, which are represented in the emotional space PAD. Using a social emotional model within intelligent entities allows the creation of more real simulations, in which emotional states can influence decision-making. The result of this social emotional mode is represented by a series of examples, which are intended to represent a number of situations in which the emotions of each individual modify the emotion of the group. Moreover, the paper introduces an example which employs the proposed model in order to learn and predict future actions trying to influence in the social emotion of a group of people.},
  langid = {english},
  options = {useprefix=true}
}



